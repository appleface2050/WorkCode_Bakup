    	{
    		"path": "/cleaner/update",
   			"description": "重命名净化器",
   			"operations": [
   				{
   					"nickname": "cleanerUpdate",
   					"summary": "重命名净化器",
                    "method": "POST",
                    "parameters": [
                    	{
                    		"name": "token",
                    		"description": "用户token",
                    		"required": true,
                    		"paramType": "form",
