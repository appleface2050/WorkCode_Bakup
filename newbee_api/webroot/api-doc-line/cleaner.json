    		"path": "/cleaner/index/token/{token}",
   			"description": "返回全部净化器",
   			"operations": [
   				{
   					"nickname": "cleanerIndex",
   					"summary": "返回全部净化器",
                    "method": "GET",
                    "parameters": [
                    	{
                    		"name": "token",
                    		"description": "用户token",
                    		"required": true,
                    		"paramType": "path",
                    		"type": "string"
                    	}
                    ]
   				}
   			]
    	},
    	 {
    		"path": "/cleaner/create",
   			"description": "添加净化器",
   			"operations": [
   				{
   					"nickname": "cleanerCreate",
   					"summary": "添加净化器",
                    "method": "POST",
                    "parameters": [
                    	{
                    		"name": "token",
                    		"description": "用户token",
                    		"required": true,
                    		"paramType": "form",
                    		"type": "string"
                    	},
                    	{
                    		"name": "id",
                    		"description": "净化器id",
                    		"required": true,
                    		"paramType": "form",
                    		"type": "string"
                    	},
                    	{
                    		"name": "qrcode",
                    		"description": "二维码上的序列号",
                    		"required": true,
                    		"paramType": "form",
                    		"type": "string"
                    	},
                    	{
                    		"name": "wifi_name",
                    		"description": "wifi用户名",
                    		"required": false,
                    		"paramType": "form",
                    		"type": "string"
                    	},
                    	{
                    		"name": "wifi_pwd",
                    		"description": "wifi密码",
                    		"required": false,
                    		"paramType": "form",
                    		"type": "string"
                    	},
                    	{
                    		"name": "name",
                    		"description": "用户命名的净化器名称",
                    		"required": true,
                    		"paramType": "form",
                    		"type": "string"
                    	},
                    	{
                    		"name": "point_x",
                    		"description": "接入所在地经度",
                    		"required": false,
                    		"paramType": "form",
                    		"type": "string"
                    	},
                    	{
                    		"name": "point_y",
                    		"description": "接入所在地纬度",
                    		"required": false,
                    		"paramType": "form",
                    		"type": "string"
                    	}
                    ]
   				}
   			]
    	},
