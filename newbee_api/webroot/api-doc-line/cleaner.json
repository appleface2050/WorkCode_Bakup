    	{
    		"path": "/cleaner/check/token/{token}/qrcode/{qrcode}",
   			"description": "检测净化器是否需要简化添加流程",
   			"operations": [
   				{
   					"nickname": "cleanerCheck",
   					"summary": "检测净化器是否需要简化添加流程需要 simple=1 else simple=0",
                    "method": "GET",
                    "parameters": [
                    	{
                    		"name": "token",
                    		"description": "用户token",
                    		"required": true,
                    		"paramType": "path",
                    		"type": "string"
                    	},
                    	{
                    		"name": "qrcode",
                    		"description": "净化器二维码序列号",
                    		"required": true,
                    		"paramType": "path",
                    		"type": "string"
                    	}
                    ]
   				}
   			]
    	},
    	{
    		"path": "/cleaner/simple",
   			"description": "净化器添加简化版",
   			"operations": [
   				{
   					"nickname": "cleanerSimple",
   					"summary": "净化器添加简化版",
                    "method": "POST",
                    "parameters": [
                    	{
                    		"name": "token",
                    		"description": "用户token",
                    		"required": true,
                    		"paramType": "form",
                    		"type": "string"
                    	},
                    	{
                    		"name": "qrcode",
                    		"description": "净化器序列号(二维码)",
                    		"required": true,
                    		"paramType": "form",
                    		"type": "string"
                    	},
                    	{
                    		"name": "name",
                    		"description": "用户定义的净化器名称",
                    		"required": true,
                    		"paramType": "form",
                    		"type": "string"
                    	}
                    ]
   				}
   			]
    	},
    	{
    		"path": "/cleaner/upgradeStatus",
   			"description": "升级状态",
   			"operations": [
   				{
   					"nickname": "cleanerUpgradeStatus",
   					"summary": "净化器固件升级状态",
                    "method": "POST",
                    "responseClass": "Return",
                    "notes": "<p>调用请求升级的接口后, 每隔3-5秒,轮询下该接口,查看是否更新完成, 更新完成后, 不再轮询</p>",
                    "parameters": [
                    	{
                    		"name": "token",
                    		"description": "用户token",
                    		"required": true,
                    		"paramType": "form",
                    		"type": "string"
                    	},
                    	{
                    		"name": "id",
                    		"description": "净化器id",
                    		"required": true,
                    		"paramType": "form",
                    		"type": "string"
                    	}
                    ]
   				}
   			]
    	}
    ],
    "models": {
    	"Return": {
    		"id" : "Return",
    		"properties": {
    			"status": {
    				"type": "integer",
    				"description": "成功失败标识,如果成功则为1,失败则为0",
    				"default": "1",
    				"required": true
    			},
    			"msg": {
    				"type": "string",
    				"description": "提示信息,如果成功则为空字符串,失败则为失败的提示信息",
    				"required": true
    			},
    			"data": {
    				"$ref": "Finish"
    			}
    		}
    	},
    	"Finish": {
    		"id" : "Finish",
    		"properties": {
    			"finish": {
    				"type": "integer",
    				"description": "是否完成更新 1完成 0未完成",
    				"default": 1,
    				"required": true
    			}
    		}
    	}
    }
}