{
    "swaggerVersion": "1.0",
    "basePath": "http://www.cleaner.com/upgrade",
    "apiVersion": "1.0",
    "resourcePath": "/upgrade",
	"apis": 
    [
    	{
    		"path": "/version/create",
   			"description": "添加升级程序",
   			"operations": [
   				{
   					"nickname": "versionCreate",
   					"summary": "添加新版本",
                    "method": "POST",
                    "responseClass": "Return",
                    "notes": "<p>硬件端根据自己的需求上传新版本程序</p>",
                    "parameters": [
                    	{
                    		"name": "auth",
                    		"description": "服务器端分配的一个值",
                    		"required": "true",
                    		"paramType": "form",
                    		"type": "string"
                    	},
                    	{
                    		"name": "version",
                    		"description": "要升级成的版本号",
                    		"required": "true",
                    		"paramType": "form",
                    		"type": "string"
                    	},
                    	{
                    		"name": "client_id",
                    		"description": "客户端id(PS:100表示瑞和     300 圆形机器)",
                    		"required": "true",
                    		"paramType": "form",
                    		"type": "integer",
                    		"defaultValue": "100",
                    		"enum":[100,300,201,110,310]
                    	},
                    	{
                    		"name": "description",
                    		"description": "升级描述信息(没有可以不填,最好填写,做为备忘)",
                    		"required": "false",
                    		"paramType": "form",
                    		"type": "string",
                    		"defaultValue": "程序升级"
                    	},
                    	{
                    		"name": "filepath",
                    		"description": "升级文件",
                    		"required": "true",
                    		"paramType": "body",
                    		"type": "file"
                    	}
                    ]
               }
            ]
    	}
    ],
    "models": {
    	"Return": {
    		"id" : "Return",
    		"properties": {
    			"status": {
    				"type": "integer",
    				"description": "成功失败标识,如果成功则为1,失败则为0",
    				"default": "1",
    				"required": true
    			},
    			"msg": {
    				"type": "string",
    				"description": "提示信息,如果成功则为空字符串,失败则为失败的提示信息",
    				"required": true
    			},
    			"data": {
    				"type": "array",
    				"items" : {
    					"type" : "string",
    					"default": ""
    				},
    				"description": "无论成功失败,都是一个空数组,此接口该字段忽略不计"
    			}
    		}
    	},
    	"VersionReturn": {
    		"id" : "VersionReturn",
    		"properties": {
    			"status": {
    				"type": "integer",
    				"description": "成功失败标识,如果成功则为1,失败则为0",
    				"default": "1",
    				"required": true
    			},
    			"msg": {
    				"type": "string",
    				"description": "提示信息,如果成功则为空字符串,失败则为失败的提示信息",
    				"required": true
    			},
    			"data": {
    				"$ref": "Version",
    				"description": "无论成功失败,都是一个空数组,此接口该字段忽略不计"
    			}
    		}
    	},
    	"Version":{
    		"id" : "Version",
    		"properties": {
    			"update": {
    				"type": "integer",
    				"description": "是否更新(1更新0不更新)",
    				"default": "0",
    				"required": true
    			},
    			"force_update": {
    				"type": "string",
    				"description": "如果更新的话,是否强制更新(1强制0不强制)",
    				"required": true
    			},
    			"update_url": {
    				"type": "string",
    				"description": "更新地址",
    				"required": true
    			},
    			"title": {
    				"type": "string",
    				"description": "标题",
    				"required": true
    			},
    			"info": {
    				"type": "string",
    				"description": "更新描述",
    				"required": true
    			}
    		}
    	}
    }
 }