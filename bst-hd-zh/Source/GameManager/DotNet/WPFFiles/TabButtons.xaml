<UserControl x:Class="BlueStacks.hyperDroid.GameManager.TabButtons"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BlueStacks.hyperDroid.GameManager"
             mc:Ignorable="d" 
             d:DesignHeight="32" d:DesignWidth="302">
    <Grid  x:Name="TabGrid">
        <Grid.Background>
            <LinearGradientBrush StartPoint="0.5,1" EndPoint="0.5,0" >
                <GradientStop Color="#51505C" Offset="0"/>
                <GradientStop Color="#3e3c4c" Offset="1"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Grid.ColumnDefinitions>

        </Grid.ColumnDefinitions>
    </Grid>
</UserControl>
