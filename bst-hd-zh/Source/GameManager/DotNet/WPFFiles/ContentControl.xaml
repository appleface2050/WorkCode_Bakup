<UserControl x:Class="BlueStacks.hyperDroid.GameManager.ContentControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BlueStacks.hyperDroid.GameManager"
             mc:Ignorable="d"
             d:DesignHeight="54" d:DesignWidth="96" 
             x:Name="contentControl"
             Margin="0,-1,0,0">
    <Grid>
        <Border x:Name="ContentControlBorder" BorderThickness="1" BorderBrush="Transparent">
            <Grid >
                <Grid x:Name="ControlGrid">
                    <Grid x:Name="FrontendGrid">
                    </Grid>
                    <Grid x:Name="ProgressBarGrid">
                        <local:ProgressBar x:Name="mProgressBarControl" />
                    </Grid>
                </Grid>
            </Grid>
        </Border>
    </Grid>
</UserControl>
