
       <div class="ztlistbox"><img src="<%= dataresult.result.topic.big_image_url %>"></div>
               <div class="app_conTitle"><a class="app_titleText" ><%= dataresult.result.topic.topic_name %></a></div>
			 
			   <div class="app_conbox conbox appgamelist appztlist" >
				<div class="appgamedlbox">
				<% for(var j=0;j<dataresult.result.topic_game.length; j++){ 
				console.log(dataresult.result.topic_game[j].icon_url);
				%>
				  <dl>
				  	<dt>
						<a onclick="browser2Client('showdetails','app_detail_html?id=<%= dataresult.result.topic_game[j].id%>','');_hmt.push(['_trackEvent', '专题_<%=dataresult.result.topic.topic_name  %>_<%=j+1 %>','click'])"><div class='mask_white '></div></a>
							<% 
							if(dataresult.result.topic_game[j].icon_url==null||dataresult.result.topic_game[j].icon_url==""||dataresult.result.topic_game[j].icon_url=="None"){
								var arr_down=['unknown',dataresult.result.topic_game[j].title,dataresult.result.topic_game[j].quick_download_download_url];
							}else{
								var arr_down=[dataresult.result.topic_game[j].icon_url,dataresult.result.topic_game[j].title,dataresult.result.topic_game[j].quick_download_download_url];
							} %>
						<div class='az_btn' onclick="browser2Client('installapp','<%=arr_down%>','<%=dataresult.result.topic_game[j].quick_download_package_name%>');_hmt.push(['_trackEvent', '专题_<%=dataresult.result.topic.topic_name  %>_<%=j+1 %>_一键安装','click'])">一键安装</div>
					    <img src=" <%= dataresult.result.topic_game[j].icon_url %> ">
					</dt>
					<a onclick="browser2Client('showdetails','app_detail_html?id=<%= dataresult.result.topic_game[j].id%>','');_hmt.push(['_trackEvent', '专题_<%=dataresult.result.topic.topic_name  %>_<%=j+1 %>','click'])">
						<dd class="app_name"><%= dataresult.result.topic_game[j].title %></dd>
					</a>
						<dd class="app_star"> 
						<% 	var array_picture = [{0:'stars_0'},{0.5:'stars_1'},{1:'stars_2'},{1.5:'stars_3'},{2:'stars_4'}];
							var score=dataresult.result.topic_game[j].level;
							//alert(score);
							var star_array=[];
							var star_pic='';
							for(var i=0; i<5; i++){
								if(score>2){
									star_array.push(2);
									score-=2;
								}else{
									star_array.push(score);
									score=0;
								}
							}
							$.each(star_array,function(){
								if (this <= 0)
								{
									star_pic = array_picture[0][0];							
								}
								else if (this <= 0.5)
								{
									star_pic = array_picture[1][0.5];
								}
								else if (this < 1.5)
								{
									star_pic = array_picture[2][1];
								}
								else if (this < 2)
								{
									star_pic = array_picture[3][1.5];
								}
								else if (this >= 2)
								{
									star_pic = array_picture[4][2];
								}
							%>	
								<img src='../../static/images/appcenter/<%= star_pic %>.png'>
								
							<% });%>
						
						</dd>
						<dd class="app_type">
						<%	
						if(dataresult.result.topic_game[j].type!="" && dataresult.result.topic_game[j].type instanceof Array){
							$.each(dataresult.result.topic_game[j].type,function(){ %>
								<a href='app_center_tag_html?tag=<%= this %>'><span><%= this %></span></a>
						<% })
						} %>
						</dd>
						<dd class="app_down"><span><%= dataresult.result.topic_game[j].download_count %></span>+次下载</dd>
						
					
					
				  </dl>
				 <% } %>
				 </div></div>
				 
